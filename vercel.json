{
    "builds":[{
        "src": "config/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb","runtime":"python3.12"}
    },
    {
        "src": "run.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "ui/staticfiles"
        }    
    }],
    "routes": [
        {"src": "/static/(.*)", "dest": "/static/$1"},
        {"src": "/(.*)", "dest": "config/wsgi.py"}]
}